<CamClick>:
    name: 'camClick'
    canvas:
        Color:
            rgba: 1,1,1
        Rectangle:
            pos: self.pos
            size: self.size
    BoxLayout:
        FloatLayout:
            Camera:
                id: camera
                resolution: (2160,3088)
            ToggleButton:
                text: 'Play'
                size_hint: .1, .1
                pos_hint: {'center_x':.3, 'center_y': .035}
                on_press: camera.play = not camera.play
                background_normal: 'play.png'
            Button:
                pos_hint: {'center_x':.45, 'center_y': .035}
                size_hint: .1, .1
                background_normal: 'camara.png'
                on_press: self.parent.parent.parent.capture()
            Button:
                id: bt_inicio
                size_hint: .07, .07
                pos_hint: {'center_x':.94, 'center_y': .035}
                background_normal: 'inicio.png'
                on_press: app.root.menuP()
            Button:
                id: bt_cancelar
                size_hint: .06, .075
                pos_hint: {'center_x':.6, 'center_y': .035}
                background_normal: 'cancelar.png'
                on_press: app.root.opciones_mercancias()