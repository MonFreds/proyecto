        FloatLayout:
            AsyncImage:
                source: 'pizap.com15234302397901.jpg'
                size_hint: 1.1,1
                pos_hint: {'center_x':.5, 'center_y': .5}

            Button:
                text: 'MERCANCIA'
                size_hint: .3, .3
                pos_hint: {'center_x':.19, 'center_y': .4}
                background_normal: 'mercancia.png'
                on_press:app.root.camaraAccion()

            Button:
                id:tb_proveedores
                text: 'PROVEEDORES'
                size_hint: .25, .3
                pos_hint: {'center_x':.27, 'center_y': .75}
                background_normal: 'proveedor.png'
                on_press:app.root.opciones_proveedores()

            Button:
