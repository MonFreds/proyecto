<CustomLayout>
    canvas:
        Color:
            rgba: 1,1,1
        Rectangle:
            pos: self.pos
            size: self.size

<Cargar>
    name:'cargar'
    orientation: "vertical"
    BoxLayout:
        FloatLayout:
            Button:
                background_normal: 'pizap.com15234302397901.jpg'
                size_hint:1,1
                pos_hint: {'center_x':0.5, 'center_y': 0.5}
                on_press: app.root.login()

<Inicio>
    name: 'inicio'
    orientation: "vertical"
    BoxLayout:
        FloatLayout:
            AsyncImage:
                source: 'inicio.jpg'
                size_hint: 1,1.2
                pos_hint: {'center_x':0.5, 'center_y': 0.5}

            Button:
                id: bt_inicio
                text: 'Inicio'
                font_size: 23
                size_hint: .4, .12
                pos_hint: {'center_x':.2, 'center_y': .12}
                background_normal:'boton.jpg'
                on_press:self.parent.parent.parent.validarIni()

            Label:
                text: 'BIENVENIDO'
                font_size: 70
                pos_hint: {'center_x': .58, 'center_y': 0.85}

            Label:
                text: 'Login: '
                font_size: 20
                text_size: self.width+50, self.height+50
                pos_hint: {'center_x': .84, 'center_y': 1.08}

            TextInput:
                id: ti_userL
                hint_text: "Ingresa Login"
                multiline: False
                size_hint: .28,.05
                font_size: 16
                pos_hint: {'center_x': .61, 'center_y': .57}

            Label:
                text: 'Password:'
                font_size: 20
                text_size: self.width+50, self.height+50
                pos_hint: {'center_x': .84, 'center_y': .98}

            TextInput:
                id: ti_passwordL
                hint_text: "Ingresa Password"
                multiline: False
                size_hint: .28,.05
                font_size: 16
                pos_hint: {'center_x': .61, 'center_y': .47}
                password: True

            Button:
                id: bt_salir
                text: 'SALIR'
                size_hint: .06, .07
                pos_hint: {'center_x':.96, 'center_y': .04}
                background_normal: 'salir.png'
                on_press:  app.stop()


<ScreenManagement>:

    Cargar:
    Inicio:
    PaginaInicial:
    LabelConfig:
    CamClick:

<PaginaInicial>:
    name: 'paginaInicial'
